parameter[p0c] = 5e9
bmad_com[radiation_damping_on] = T

q1: quad, l = 0.5, k1 = 0.6
q2: quad, l = 0.5, k1 = -0.5

d: drift, l = 0.3
b: sbend, l = 3, angle = pi/8, e1 = 0.07, e2 = 0.07
b2: sbend, l = 0.01, angle = pi/500, num_steps = 10

s1: sextupole, l = 0.01
s2: sextupole, l = 0.01

rf: rfcavity, l = 1.2, rf_frequency = 250e6, voltage = 1e6, phi0 = 0.18

sector: line = (b2, b, s1, d, q1, d, b, d, q2, d, s2, rf)
ring: line = (sector)

use, ring
