no_digested
parameter[e_tot] = 1e9
parameter[n_part] = 1e10
parameter[geometry] = closed
beginning[beta_a] = 10
beginning[beta_b] = 10

q1: quad, l = 10, wall = {thickness = 0.1, 
    superimpose = F, ele_anchor_pt = center, 
    section = {type = opaque, s = 1, x0 = 1, y0 = 2, dr_ds = 10, 
      v(1) = {11, 12, 130, 140}, 
      v(2) = {1, 2, 30, 40, 5}, 
      v(3) = {-21, 22} &
    },
    section = {s = 2, v(1) = {1,1}},
    section = {s = 5, v(1) = {1,1}} &
} 

q2: quad, l = 10, wall = {
    section = {s = 2, v(1) = {1,1}},
    section = {s = 5, v(1) = {1,1}} &
  }    


q3: q2

m: marker, superimpose, ref = q2, wall = {superimpose = T,
    section = {s = -1, v(1) = {1,1}},
    section = {s = 1, v(1) = {1,1}} &
  }

m2: marker, wall = {superimpose = T,
    section = {s = -1, v(1) = {1,1}},
    section = {s = 1, v(1) = {1,1}} &
  }

l1: line[multipass] = (q1, q2)
l2: line = (l1, l1, m2)
use, l2
