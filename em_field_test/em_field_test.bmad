no_digested
beginning[beta_a] = 10
beginning[beta_b] = 10
parameter[e_tot] = 1e9
parameter[geometry] = open
beam_start[x] = 0.1
beam_start[y] = 0.2
beam_start[z] = 0.3
beam_start[px] = 1e-5


q: quadrupole, field_calc = map, l = 2, 
  field = {
    mode = {
    m = 2,
    map = {dz = 1,
           e_coef_re = ( 3, 2),
           e_coef_im = ( 1, -1),
           b_coef_re = ( 0, 0),
           b_coef_im = ( 0, -1) } } }

pillbox_cav: lcavity, tracking_method = runge_kutta, gradient=10e6, mat6_calc_method = tracking, field_calc = map,
  rf_frequency = 1.3e9, L = 0.1,
  field = {
    mode = {
    m = 0,
    harmonic = 1,
    map = {dz =   0.1 ,
           e_coef_re = ( 1, 0),
           e_coef_im = ( 0, 0) } } }


w1: wiggler, l= 1, tracking_method = symp_lie_bmad, num_steps = 10, &
     term(001)={1.0e-3, 4, 5, 3, 0.2, 0.1, 2, y}

w2: wiggler, l= 1, tracking_method = symp_lie_bmad, num_steps = 10, &
     term(001)={1.0e-3, 3, 4, 5, 0.2, 0.1, 2, x}

w3: wiggler, l= 1, tracking_method = symp_lie_bmad, num_steps = 10, &
     term(001)={1.0e-3, 5, 4, 3, 0.2, 0.1, 0.2, x}




ll: line = (w1)
use, ll
