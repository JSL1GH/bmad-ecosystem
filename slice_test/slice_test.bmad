no_digested

beginning[beta_a]  = 10  
beginning[beta_b]  = 12

parameter[lattice_type] = linear_lattice
beginning[e_tot]  = 7e6
beam_start[x] = 0e-4
beam_start[px] = 1e-4

!

bend: sbend, rho = 1, angle = pi/10, y_pitch = 1e-4, tilt = 1.5, roll = 2e-4,
          x_offset = 0.1, y_offset = 0.2
bend2: bend
m: marker, superimpose, ref = bend, offset = 0.1

lat: line = (bend, bend2)

! branch 1


sol: solenoid, ks = 0.3, l = 1
s: sextupole, k2 = 0.7, l = 1, ds_step = 0.01
b: sbend, angle = 0.3, l = 1, e1 = 0.1

lat2: line = (sol, s, b)
lat2[e_tot] = 7e6
lat2[beta_a]  = 10  
lat2[beta_b]  = 12
lat2[eta_x]   =  1.3
lat2[etap_x]  =  0.1

!

use, lat, lat2

