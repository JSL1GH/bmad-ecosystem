&parameters
  !See ibs_ring.f90 for documentation
  lat_file = 'lat/ideal_and_tunes.lat'
  ibs_formula = 'kubo'
  eqb_method = 'der'
  clog_to_use = 3   ! 1=no tail cut, 2=raubenheimer, 3=kubo, 4=kubo w/ vertical
  inductance = 25.9E-9
  resistance = 1500.0

  set_dispersion = .true.   !applied only for kubo method.
  eta_set = 0.010
  etap_set = 0.002

  b_emit = 17.90E-12  ! 16.9E-12
  a_emit = 3.8E-9
  energy_spread = -1

  ratio = 0.0  ! Coupling 'r' parameter.  Used only if eqb_method set to 'rlx'.

  granularity = -1.0   ! Step size in meters.  -1 for element-by-element.

  x_view = 479    !vBSM positrons   !element number in lattice where horizontal and longitudinal projection to be taken.
  y_view = 939    !xBSM positrons (dline)
  z_view = 479    !vBSM positrons

  mA_per_bunch = 8.0   !highest current
  stop_mA = 0.01  !lowest current
  delta_mA = 0.40  !step size
&end
