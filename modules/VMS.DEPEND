$(MOD_OUT_DIR)bmad_interface.f90$$mod:  $(MOD_OUT_DIR)bmad_struct.f90$$mod
$(LIBRARY)(bmad_interface.obj):         $(MOD_OUT_DIR)bmad_struct.f90$$mod

$(MOD_OUT_DIR)bmad_struct.f90$$mod:  $(MOD_OUT_DIR)physical_constants.f90$$mod
$(LIBRARY)(bmad_struct.obj):         $(MOD_OUT_DIR)physical_constants.f90$$mod

$(MOD_OUT_DIR)local_bmad_interface.f90$$mod:  $(MOD_OUT_DIR)bmad_interface.f90$$mod $(MOD_OUT_DIR)local_bmad_struct.f90$$mod
$(LIBRARY)(local_bmad_interface.obj):         $(MOD_OUT_DIR)bmad_interface.f90$$mod $(MOD_OUT_DIR)local_bmad_struct.f90$$mod

$(MOD_OUT_DIR)local_bmad_struct.f90$$mod:  $(MOD_OUT_DIR)bmad_struct.f90$$mod
$(LIBRARY)(local_bmad_struct.obj):         $(MOD_OUT_DIR)bmad_struct.f90$$mod

$(MOD_OUT_DIR)rad_int_common.f90$$mod:  $(MOD_OUT_DIR)bmad_struct.f90$$mod
$(LIBRARY)(rad_int_common.obj):         $(MOD_OUT_DIR)bmad_struct.f90$$mod

